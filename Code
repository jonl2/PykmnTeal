                room = 'pallet_lab'
                ps("You notice %s is inside already.\n%s! %s! I have three POKéMON here on the desk.\n%s, since I promised a POKéMON to you,\nI'll let you pick first!"%(rname,name,rname,name))
                ps('There are three pokemon to choose from:\n')
                ps('CHARMANDER - the blaze pokemon. A strong fire pokemon.\n')
                ps('SQUIRTLE - the turtle pokemon. A strong water pokemon.\n')
                ps('BULBASAUR - the vine pokemon. A strong grass pokemon.\n')
                ps('Choose one: C for Charmander, S for Squirtle, and B for Bulbasaur.\n')
                choice = input(': ').upper() #Choose starter pkmn.

                if choice == 'C':
                    pkm1 = 'Charmander'
                    ID = 4
                if choice == 'S':
                    pkm1 = 'Squirtle'
                    ID = 7
                if choice == 'B':
                    pkm1 = 'Bulbasaur'
                    ID = 1

    
#Vars
belt = [] #Pokemon belt
bag = {pokeballs:{'pokeball':0,'great ball':0},
       medicine:{'potion':0,'super potion':0,'hyper potion':0,'max potion':0}
       }

#Key commands
delay = 0
z = 1
ec = 1

currentroom = 0
a = event_1()
gender = a[0]
name = a[1]
rname = a[2]
timesleep()
ps("\nOh! %s! Don't forget your stuff before you go out!\n"%(name))
time.sleep(.5)
ps('Obtained BACKPACK!\n')
time.sleep(.5)
ps('Obtained TRAINER CARD\n')
time.sleep(.5)
ps('Go! Your adventure awaits!\n')
Help()
while z == 1:
     
    action = input(': ').lower()
    if action == 'x' or action == 'menu':
        menu()
    elif ec == 2:
        event_2(room[currentroom]['name'])
    #move actions
    elif action == 'type' or action == 'east' or action == 'north' or action =='south' or action =='west' or action =='name':
        newroom = room[currentroom][action]
        if newroom == -1:
            print('You Cant Go That Way')
        if newroom != -1:
            if action != 'type':
                if action != 'desc':
                    currentroom=newroom
                    print('\n\n')
                    print(room[currentroom]['desc'])
    else:
        print('You Can\'t Do That')
    
    
    

    
    

