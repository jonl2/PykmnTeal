#Misc Functions
import time
import random
def ps(str): #slow typing for single strs.
    for letter in str:
        print (letter, end='')
        time.sleep(delay)

def timesleep(): # Triple dot print with time interval
    time.sleep(1)
    print('.')
    time.sleep(1)
    print('.')
    time.sleep(1)
    print('.')
    time.sleep(1)

def Help(): # Description of all availible commands.
    ps('To move between rooms, use NORTH, EAST, SOUTH WEST.\nThey do not have to be capitalized. NoRtH or nOrTH will work as well.\n')

#Pokemon Generation
class create_pokemon(object):
    Type = 'none'
    def __init__(self,name,base,IV,exp,level,ID,Nat,EV): #This calculates the values of stats.
        self.name = name
        self.levelup(base,EV,IV,level,exp,Nat)
        self.level = level #Pokemon level
        self.ID = ID #ID number associated with the pkmn
        self.HP = self.orHP #HP number that is used for battle and the constantly fluctuating one.
        self.Type = base[9] #Type based off of base stats of each pkmn in "base" list.
        self.IV = IV #Random IV numbers to make each pokemon "unique". Based off randomization.
        self.Nat = Nat #Nature that determines slight increases in particular stats.
        self.status = 'none'
        print(self.name,self.attack,self.defense,self.spattack,self.spdef,self.speed,self.orHP,self.exp,self.level,self.ID,self.Type,)

    def levelup(self,base,EV,IV,level,exp,Nat):
        self.level = level
        
        self.attack = int(round(((((2*base[1]+IV[0]+(EV/4))*level)/100)+5)*Nat[0])) #The equation for attack
        self.defense = int(round(((((2*base[2]+IV[1]+(EV/4))*level)/100)+5)*Nat[1])) #The equation for defense
        self.spattack = int(round(((((2*base[3]+IV[2]+(EV/4))*level)/100)+5)*Nat[2])) #The equation for special attack
        self.spdef = int(round(((((2*base[4]+IV[3]+(EV/4))*level)/100)+5)*Nat[3])) #The equation for special defense
        self.speed = int(round(((((2*base[5]+IV[4]+(EV/4))*level)/100)+5)*Nat[4])) #The equation for speed
        self.orHP = int(round(((2*base[6]+IV[5]+(EV/4))*level)/100)+level+10) #The equation for health points (HP)
        self.exp = exp

    def evolup(self,ID,base,EV,level):
        self.ID = self.ID + 1
        self.levelup()
        None
    def bat_init_(self):
        self.crit = 0
        self.acc = 0
        self.eva = 0
        self.atkstg = 0
        self.defstg = 0
        self.spatkstg = 0
        self.spdefstg = 0
        self.spdstg = 0

    def status(self):
        self.batatk = self.attack
        self.batdef = self.defense
        self.batspatk = self.spattack
        self.batspdef = self.spdef
        self.batspd = self.speed
        
    def faint(self):
        if self.HP == 0:
            self.faint = 1

def IVcreate():
    b = []
    count = 0
    while count < 6:
        a = random.randint(0,31)
        b.append(a)
        count = count + 1
    return b
def Natcreate():
    a = random.randint(1,25)
    nateffect = {1:('Hardy',(1,1,1,1,1)),2:('Lonely',(1.1,0.9,1,1,1)),3:('Adamant',(1.1,1,0.9,1,1)),4:('Naughty',(1.1,1,1,0.9,1)),
                 5:('Brave',(1.1,1,1,1,0.9)),6:('Bold',(0.9,1.1,1,1,1)),7:('Docile',(1,1,1,1,1)),8:('Impish',(1,1.1,0.9,1,1)),
                 9:('Lax',(1,1.1,1,0.9,1)),10:('Relaxed',(1,1.1,1,1,0.9)),11:('Modest',(0.9,1,1.1,1,1)),12:('Mild',(1,0.9,1.1,1,1)),
                 13:('Bashful',(1,1,1,1,1)),14:('Rash',(1,1,1.1,0.9,1)),15:('Quiet',(1,1,1.1,1,0.9)),16:('Calm',(0.9,1,1,1.1,1)),
                 17:('Gentle',(1,0.9,1,1.1,1)),18:('Careful',(1,1,0.9,1.1,1)),19:('Quirky',(1,1,1,1,1)),20:('Sassy',(1,1,1,1.1,0.9)),
                 21:('Timid',(0.9,1,1,1,1.1)),22:('Hasty',(1,0.9,1,1,1.1)),23:('Jolly',(1,1,0.9,1,1.1)),24:('Naive',(1,1,1,0.9,1.1)),
                 25:('Serious',(1,1,1,1,1))}
    c = nateffect[a]
    return c

